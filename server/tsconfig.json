{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "src/*"
        ]
      }
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }